{
  "name": "tlicycling-static",
  "version": "0.1.0",
  "description": "The TLI Cycling website build using Eleventy and deployed to Netlify.",
  "scripts": {
    "build": "npx eleventy",
    "clean": "rm -rf dist",
    "watch": "npx eleventy --watch",
    "debug": "DEBUG=* npx eleventy",
    "serve": "light-server -c .lightserverrc",
    "dev": "NODE_ENV=development npm-run-all clean build --parallel watch serve --print-label"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/andrewdisley/tlicycling-static"
  },
  "author": {
    "name": "Andrew Disley",
    "email": "andrew@simplified.co.uk",
    "url": "https://simplified.co.uk/"
  },
  "homepage": "https://tlicycling.com/",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "luxon": "^1.24.1",
    "markdown-it": "^11.0.0",
    "markdown-it-anchor": "^5.3.0",
    "markdown-it-attrs": "^3.0.3"
  },
  "dependencies": {
    "@11ty/eleventy-navigation": "^0.1.6",
    "clean-css": "^4.2.3",
    "html-minifier": "^4.0.0",
    "light-server": "^2.8.0",
    "npm-run-all": "^4.1.5",
    "uglify-es": "^3.3.9"
  }
}
