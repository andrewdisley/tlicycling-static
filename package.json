{
  "name": "tlicycling-static",
  "version": "1.0.0",
  "description": "The TLI Cycling website build using Eleventy and deployed to Netlify.",
  "main": "",
  "scripts": {
    "build": "npx @11ty/eleventy",
    "clean": "rm -rf dist",
    "watch": "npx @11ty/eleventy --watch",
    "debug": "DEBUG=* npx @11ty/eleventy",
    "serve": "light-server -c .lightserverrc",
    "dev": "NODE_ENV=development npm-run-all clean --parallel watch serve --print-label"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/andrewdisley/tlicycling-static"
  },
  "author": {
    "name": "Andrew Disley",
    "email": "andrew@simplified.co.uk",
    "url": "https://simplified.co.uk/"
  },
  "homepage": "https://tlicycling.com/",
  "devDependencies": {
    "light-server": "^2.9.1",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "@11ty/eleventy": "^1.0.2",
    "@11ty/eleventy-navigation": "^0.3.5",
    "clean-css": "^5.3.1",
    "eleventy-plugin-helmet": "^0.2.2",
    "eleventy-plugin-pwa": "^1.0.8",
    "html-minifier": "^4.0.0",
    "luxon": "^3.1.0",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.5",
    "markdown-it-attrs": "^4.1.4",
    "uglify-es": "^3.3.9"
  }
}
