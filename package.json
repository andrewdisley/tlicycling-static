{
  "name": "tlicycling-static",
  "version": "0.1.0",
  "description": "The TLI Cycling website build using Eleventy and deployed to Netlify.",
  "scripts": {
    "build": "npx eleventy",
    "clean": "rm -rf dist",
    "watch": "npx eleventy --watch",
    "debug": "DEBUG=* npx eleventy",
    "serve": "light-server -c .lightserverrc",
    "dev": "NODE_ENV=development npm-run-all clean --parallel watch serve --print-label"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/andrewdisley/tlicycling-static"
  },
  "author": {
    "name": "Andrew Disley",
    "email": "andrew@simplified.co.uk",
    "url": "https://simplified.co.uk/"
  },
  "homepage": "https://tlicycling.com/",
  "devDependencies": {
    "light-server": "^2.9.1",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@11ty/eleventy-navigation": "^0.1.6",
    "clean-css": "^4.2.3",
    "eleventy-plugin-helmet": "^0.2.1",
    "eleventy-plugin-pwa": "^1.0.8",
    "html-minifier": "^4.0.0",
    "luxon": "^1.25.0",
    "markdown-it": "^12.0.3",
    "markdown-it-anchor": "^6.0.1",
    "markdown-it-attrs": "^3.0.3",
    "uglify-es": "^3.3.9"
  }
}
